{
    "name": "codingame/codingame",
    "version": "0.0.1",
    "type": "project",
    "license": "MIT",
    "description": "codingame projects",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.4|^8.0",
        "symfony/flex": "^1.11",
        "symfony/http-kernel": "^5.2",
        "symfony/config": "^5.2",
        "symfony/dependency-injection": "^5.2",
        "psr/log": "^1.1"
    },
    "require-dev": {
        "behat/behat": "^3.8",
        "phpunit/phpunit": "^9.5",
        "rregeer/phpunit-coverage-check": "^0.3.1",
        "squizlabs/php_codesniffer": "^3.5",
        "symfony/dotenv": "^5.2",
        "symfony/phpunit-bridge": "^5.2"
    },
    "autoload": {
        "psr-4": {
            "CodinGame\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CodinGame\\Tests\\": "tests/"
        }
    },
    "config": {
        "bin-dir": "bin/",
        "sort-packages": true
    },
    "scripts": {
        "auto-scripts": {
        },
        "check-style": "deploy/scripts/checkStyle.sh",
        "test": "deploy/scripts/runAllTests.sh"
    }
}
