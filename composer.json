{
    "name": "codingame/codingame",
    "version": "0.0.1",
    "type": "project",
    "license": "MIT",
    "description": "codingame projects",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.3",
        "symfony/flex": "^1.11",
        "symfony/http-kernel": "^7.1",
        "symfony/config": "^7.1",
        "symfony/dependency-injection": "^7.1",
        "psr/log": "^1.1"
    },
    "require-dev": {
        "behat/behat": "^3.14",
        "phpunit/phpunit": "^11.3",
        "rregeer/phpunit-coverage-check": "^0.3.1",
        "squizlabs/php_codesniffer": "^3.7",
        "symfony/dotenv": "^7.1",
        "symfony/phpunit-bridge": "^7.1"
    },
    "autoload": {
        "psr-4": {
            "CodinGame\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CodinGame\\Tests\\": "tests/"
        }
    },
    "config": {
        "bin-dir": "bin/",
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true
        }
    },
    "scripts": {
        "auto-scripts": {
        },
        "check-style": "deploy/scripts/checkStyle.sh",
        "test": "deploy/scripts/runAllTests.sh"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true
        }
    }
}
